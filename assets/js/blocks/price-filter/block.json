{
	"name": "woocommerce/price-filter",
	"version": "1.0.0",
	"title": "Filter by Price Inner",
	"description": "Allow customers to filter products by price range.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce" ],
	"supports": {
		"html": false,
		"multiple": false,
		"color": {
			"text": true,
			"background": false
		},
		"lock": false
	},
	"example": {
		"attributes": {
			"isPreview": true
		}
	},
	"attributes": {
		"className": {
			"type": "string",
			"default": ""
		},
		"showInputFields": {
			"type": "boolean",
			"default": true
		},
		"inlineInput": {
			"type": "boolean",
			"default": false
		},
		"showFilterButton": {
			"type": "boolean",
			"default": false
		},
		"headingLevel": {
			"type": "number",
			"default": 3
		},
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": false 
			}
		}
	},
	"textdomain": "woo-gutenberg-products-block",
	"apiVersion": 2,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
