{
	"name": "woocommerce/active-filters",
	"version": "1.0.0",
	"title": "Active Product Filters Inner",
	"description": "Show the currently active product filters. Works in combination with other filters blocks.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce" ],
	"supports": {
		"html": false,
		"multiple": false,
		"inserter": false,
		"color": {
			"text": true,
			"background": false
		},
		"lock": false
	},
	"attributes": {
		"displayStyle": {
			"type": "string",
			"default": "list"
		},
		"headingLevel": {
			"type": "number",
			"default": 3
		},
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": false 
			}
		}
	},
	"textdomain": "woo-gutenberg-products-block",
	"apiVersion": 2,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
